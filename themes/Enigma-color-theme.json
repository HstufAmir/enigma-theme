{
  "name": "Enigma",
  "include": "./dark_vs.json",
  "colors": {
    // Editor
    "editor.background": "#202020",
    "editor.foreground": "#eeeeee",

    "editorCursor.foreground": "#e2dc83",
    "editorError.foreground": "#ff7676",
    "editorWarning.foreground": "#aa671a",
    "editorLineNumber.foreground": "#868686",
    "editorGroupHeader.tabsBackground": "#303030",
    "editorWhitespace.foreground": "#868686",
    "editorCodeLens.foreground": "#868686",

    // Title Bar
    "titleBar.activeBackground": "#202020",
    "titleBar.activeForeground": "#eeeeee",

    "titleBar.inactiveBackground": "#303030",
    "titleBar.inactiveForeground": "#868686",

    // Activity Bar
    "activityBar.background": "#303030",
    "activityBar.foreground": "#eeeeee",

    "activityBar.activeBorder": "#2798cc",
    "activityBar.activeBackground": "#202020",

    "activityBar.inactiveForeground": "#868686",

    "activityBarBadge.background": "#2798cc",
    "activityBarBadge.foreground": "#eeeeee",

    // Tab
    "tab.activeBackground": "#202020",
    "tab.activeForeground": "#eeeeee",
    "tab.activeBorderTop": "#2798cc",

    "tab.inactiveBackground": "#252525",
    "tab.inactiveForeground": "#868686",

    "tab.unfocusedActiveBackground": "#252525",
    "tab.unfocusedActiveForeground": "#868686",
    "tab.unfocusedActiveBorderTop": "#eeeeee",

    "tab.unfocusedInactiveBackground": "#303030",
    "tab.unfocusedInactiveForeground": "#868686",

    // Side Bar
    "sideBar.background": "#252525",
    "sideBar.foreground": "#868686",

    "sideBarTitle.background": "#202020",
    "sideBarTitle.foreground": "#eeeeee",
    "sideBarTitle.border": "#868686",

    "sideBarSectionHeader.background": "#303030",
    "sideBarSectionHeader.foreground": "#eeeeee",

    // Status Bar
    "statusBar.background": "#303030",
    "statusBar.foreground": "#eeeeee",

    "statusBarItem.activeBackground": "#303030",
    "statusBarItem.errorBackground": "#d87070",
    "statusBarItem.errorForeground": "#202020",
    "statusBarItem.warningBackground": "#ffb45e",
    "statusBarItem.warningForeground": "#202020",

    "notificationsInfoIcon.foreground": "#2798cc",
    "notificationsWarningIcon.foreground": "#e2dc83",
    "notificationsErrorIcon.foreground": "#ff7676",

    // Terminal
    "terminal.background": "#252525",

    // Ports
    "ports.iconRunningProcessForeground": "#252525",

    // Problems
    "problemsInfoIcon.foreground": "#2798cc",
    "problemsWarningIcon.foreground": "#e2dc83",
    "problemsErrorIcon.foreground": "#ff7676",

    // Output View
    "outputView.background": "#252525",

    // Debug Console
    // "debugConsole.infoForeground":"#0400ff",
    // "debugConsole.sourceForeground":"#ff0000",
    // "debugConsole.warningForeground":"#ff8800",
    // "debugConsole.errorForeground":"#09ff00",
  },
  "tokenColors": [
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": { "foreground": "#e2dc83" }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": { "foreground": "#7affbd" }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class",
        "punctuation.separator.namespace.ruby"
      ],
      "settings": { "foreground": "#0026ff" }
    },
    {
      "name": "Control flow / Special keywords",
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.directive.using",
        "keyword.other.operator",
        "entity.name.operator"
      ],
      "settings": { "foreground": "#c371d3" }
    },
    {
      "name": "Variable name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable",
        "constant.other.placeholder"
      ],
      "settings": { "foreground": "#7ad5ff" }
    },
    {
      "name": "Parameter name",
      "scope": [
        "variable.parameter",
        "variable.language",
        "meta.block variable.other",
        "support.other.variable",
        "string.other.link",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": { "foreground": "#d87070" }
    },
    {
      "name": "Constants and enums",
      "scope": ["variable.other.constant", "variable.other.enummember"],
      "settings": { "foreground": "#2798cc" }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": ["meta.object-literal.key"],
      "settings": { "foreground": "#7ad5ff" }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": { "foreground": "#ffb45e" }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": { "foreground": "#ffb45e" }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": { "foreground": "#ff7676" }
    },
    {
      "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
      "settings": { "foreground": "#e2dc83" }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": { "foreground": "#e2dc83" }
    },
    {
      "scope": ["constant.character", "constant.other.option"],
      "settings": { "foreground": "#7aa4ff" }
    },
    {
      "scope": "constant.character.escape",
      "settings": { "foreground": "#ffb45e" }
    },
    { "scope": "entity.name.label", "settings": { "foreground": "#e2dc83" } }
  ],
  "semanticTokenColors": {
    "newOperator": "#C586C0",
    "stringLiteral": "#ff0000",
    "customLiteral": "#001aff",
    "numberLiteral": "#2600ff"
  }
}
